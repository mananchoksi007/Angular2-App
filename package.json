    "scripts": {
        "clean": "rimraf ./dist",
        "build": "webpack --env=dev --progress --profile --colors",
        "build:prod": "npm run clean && webpack --env=prod --progress --profile --colors",
        "start": "webpack-dev-server --env=dev --open --progress",
        "start:prod": "npm run clean && webpack-dev-server --env=prod --open",
        "test": "karma start ./config/karma.conf.js ",
        "webdriver-manager": "webdriver-manager update --standalone false --gecko false",
        "test:e2e": "protractor ./config/protractor.conf.js",
        "lint": "tslint 'src/**/*.ts' -c ./config/tslint.json"
    },
